  "definitions": {
    "DpiPropertiesFormat": {
      "type": "object",
      "properties": {
        "provisioningState": {
          "readOnly": true,
          "$ref": "./common.json#/definitions/ProvisioningState",
          "description": "The provisioning state of the resource."
        },
        "tetheringQualityIndexValue": {
          "type": "integer",
          "minimum": 0,
          "maximum": 100,
          "default": 70,
          "description": "Percentage value for Quality Index (Confidence Index ) to mark connection as tethered 1..100"
        },
        "tetheringQualityIndexResultWeightPercentageTtl": {
          "type": "integer",
          "minimum": 0,
          "maximum": 100,
          "default": 80,
          "description": "Percentage value for Quality Index (Confidence Index ) weight for ttl method"
        },
        "tetheringQualityIndexResultWeightPercentageUserAgent": {
          "type": "integer",
          "minimum": 0,
          "maximum": 100,
          "default": 99,
          "description": "Percentage value for Quality Index (Confidence Index ) weight for user-agent method"
        },
        "tetheringQualityIndexResultWeightPercentageTcpFingerprinting": {
          "type": "integer",
          "minimum": 0,
          "maximum": 100,
          "default": 100,
          "description": "Percentage value for Quality Index (Confidence Index ) weight for tcp-fingerprinting"
        },
        "tetheringQualityIndexResultWeightPercentageGquicUserAgent": {
          "type": "integer",
          "minimum": 0,
          "maximum": 100,
          "default": 100,
          "description": "Percentage value for Quality Index (Confidence Index ) weight for gquic-user-agent"
        },
        "tetheringDetectionAlgorithmTtl": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "enabled",
          "description": "Admin state for running tethering detection algorithm using ttl"
        },
        "tetheringDetectionAlgorithmUserAgent": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "enabled",
          "description": "Admin state for running tethering detection algorithm using user-agent"
        },
        "tetheringDetectionAlgorithmTcpFingerprinting": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "disabled",
          "description": "Admin state for running tethering detection algorithm using tcp fingerprinting - Changes to this object do not take effect until the next POD restart"
        },
        "tetheringDetectionAlgorithmGquicUserAgent": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "disabled",
          "description": "Admin state for running tethering detection algorithm using gquic useragent - Changes to this object do not take effect until the next POD restart"
        },
        "tetheringDetectionAlgorithmTtlConnectionTracking": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "enabled",
          "description": "Admin state for running tethering detection algorithm using ttl by taking connection tracking into consideration"
        },
        "tetheringDetectionAlgorithmTdci": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "disabled",
          "description": "Admin state for running tethering detection algorithm using tdci"
        },
        "tetheringDetectionTdciWeightTtl": {
          "type": "integer",
          "minimum": 0,
          "maximum": 200,
          "default": 100,
          "description": "Tethering Detection Confidence Index weight in percentage for ttl method"
        },
        "tetheringDetectionTdciWeightUserAgent": {
          "type": "integer",
          "minimum": 0,
          "maximum": 200,
          "default": 0,
          "description": "Tethering Detection Confidence Index weight in percentage for user-agent method"
        },
        "tetheringDetectionTdciWeightTcpFingerprinting": {
          "type": "integer",
          "minimum": 0,
          "maximum": 200,
          "default": 70,
          "description": "Tethering Detection Confidence Index weight in percentage for tcp-fingerprinting"
        },
        "tetheringDetectionTdciWeightGquicUserAgent": {
          "type": "integer",
          "minimum": 0,
          "maximum": 200,
          "default": 100,
          "description": "Tethering Detection Confidence Index weight in percentage for gquic-user-agent"
        },
        "attributeDetectionPacketLimit": {
          "type": "integer",
          "minimum": 0,
          "maximum": 4294967294,
          "default": 50,
          "description": "The maximum number of packets to detect flow-attribute of a dpi flow"
        },
        "attributeDetectionOnlyFirstFlowAttribute": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "disabled",
          "description": "Consider only first attribute of a flow for rule match"
        },
        "controlAppsSkipAttrCheck": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "disabled",
          "description": "Admin state to enable/disable attribute check for marking control connections in Control Flow Feature"
        },
        "optimizeDpiRules": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "enabled",
          "description": "Evaluate required rules based on reclassification event trigger"
        },
        "optimizePargRules": {
          "$ref": "./common.json#/definitions/enableDisable",
          "default": "enabled",
          "description": "Evaluate parg rules configured may match to classification event"
        },
        "tetheringDetectionPacketLimit": {
          "type": "integer",
          "minimum": 3,
          "maximum": 4294967294,
          "default": 64,
          "description": "The maximum number of packets of a flow to be analysed for tethering detection, default is 64. The minimum value that can be configured is 3"
        },
        "dpiPacketCountWithProxy": {
          "type": "integer",
          "minimum": 0,
          "maximum": 4294967294,
          "default": 50,
          "description": "Set the max packet count for dpi processing when proxy is enabled,0 is for all packets"
        }
      },
      "description": "The MobilePacketCore dpi resource definition."
    },
