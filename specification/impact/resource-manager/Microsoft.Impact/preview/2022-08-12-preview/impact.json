  "definitions": {
    "impactsResource": {
      "type": "object",
      "description": "impacts details",
      "required": ["startDateTime"],
        "properties": {
            "startDateTime": {
              "type": "string",
              "description": "Time when impact started. Mandatory"
            },
            "endDateTime": {
              "type": "string",
              "x-nullable": true,
              "description": "Time when impact ended. Optional"
            },
            "impactCategory": {
              "type": "string"
            },
            "region": {
              "type": "string",
              "description": "Azure Region where impact was observed. Mandatory"
            },
            "callerId": {
              "type": "string",
              "description": " Identification of the caller. Domain data to be onboarded per caller. Mandatory"
            },
            "workloadContext": {
              "type": "string",
              "description": "Valid context defining the kind of workload for a given caller. Domaindata to be onboarded per caller. Mandatory."
            },
            "resourceType": {
              "type": "string",
              "description": " Azure resource type that was impacted. Mandatory"
            },
            "impactType": {
              "type": "string",
              "description": "Valid impact types that scopes the customer issues. This is domain data that weexpand as we onboard new clients. Mandatory"
            },
            "resourceUriList": {
              "type": "array",
              "description": "Resource uris of impacted resources separated by semi-colon. Allresources in the batch must belong to the resource-type provided as parameter to the api.Mandatory",
              "items": {
                "type": "string"
              }
            },
            "correlationIdList": {
              "type": "string",
              "x-nullable": true,
              "description": " List of ARM correlation ids separated by semi-colon. Optional."
            },
            "operationIdList": {
              "type": "string",
              "x-nullable": true,
              "description": "List of operations ids separated by semi-colon. Operation Ids come fromAzure components like CRP, Disk. Optional."
            },
            "errorCode": {
              "type": "string",
              "x-nullable": true,
              "description": "ARM error code returned by Azure. Optional"
            },
            "errorMessage": {
              "type": "string",
              "x-nullable": true,
              "description": "ARM error message returned by Azure. Optional."
            },
            "impactDescription": {
              "type": "string",
              "x-nullable": true,
              "description": "Free text for describing the impact. Optional"
            },
            "extendedProperties": {
              "type": "object",
              "x-nullable": true,
              "description": "Free Json describing the impact. Optional"
            },
            "toolset": {
              "type": "string",
              "x-nullable": true,
              "description": " Tool used to interact with Azure. e.g. Terraform, Powershell, Portal. This is freeflowing text. Optional."
            }
        }
      },

    "impactsResourceList": {
      "description": "The list of impacts.",
      "type": "object",
      "properties": {
        "nextLink": {
          "description": "The link used to get the next page of impacts list.",
          "type": "string"
        },
        "value": {
          "description": "The list of recommendations.",
          "type": "array",
          "items": {
            "$ref": "#/definitions/impactsResource"
          }
        }
      }
