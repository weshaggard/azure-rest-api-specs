enum ProvisioningState {
  NotSpecified,
  Accepted,
  Creating,
  Deleting,
  Canceled,
  Failed,
  Succeeded,
}

enum Comparison {
  Greater,
  Less,
  Equals,
  NotEquals,
  GreaterOrEquals,
  LessOrEquals,
  Contains,
  NotContains,
  Ignore,
  Exists,
}

@withVisibility("read")
@doc("Resource base")
model Resource {
  @doc("The provisioning state of the resource.")
  @visibility("read")
  provisioningState?: ProvisioningState;
}
