{
  "name": "Microsoft.EncryptedTransport",
  "author": "encryptedtransport@microsoft.com",
  "description": "Microsoft.EncryptedTransport API definitions",
  "type": "module",
  "scripts": {
    "build": "npm run build-internal",
    "build-internal": "pwsh ./BuildRP.ps1",
    "format": "npx cadl format @.cadl",
    "wsl_install:all": "bash .init/install.sh",
    "wsl_register": "bash .init/register.sh",
    "win_install:all": "pwsh .init/install.ps1",
    "win_register": "pwsh .init/register.ps1",
    "refreshVSToken": "vsts-npm-auth -config .npmrc",
    "win_apitest:generate": "pwsh ./swagger/apitest.ps1 -command generate",
    "win_apitest:run": "pwsh ./swagger/apitest.ps1 -command run",
    "wsl_apitest:generate": "bash apitest.sh generate",
    "wsl_apitest:run": "bash apitest.sh run"
  },
  "dependencies": {
    "@azure-tools/cadl-azure-core": "^0.4.0",
    "@azure-tools/cadl-azure-resource-manager": "^0.7.0",
    "@azure-tools/cadl-providerhub": "^0.18.0",
    "@azure-tools/cadl-providerhub-controller": "^0.13.0",
    "@cadl-lang/openapi3": "^0.12.0",
    "@cadl-lang/prettier-plugin-cadl": "^0.5.12",
    "@cadl-lang/rest": "^0.15.0",
    "autorest": "^3.6.1",
    "oav": "^3.0.4"
  },
  "devDependencies": {
    "@cadl-lang/compiler": "^0.32.0"
  }
}
